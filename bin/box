#!/bin/bash
if [[ "$1" == "folders:upload" ]]; then
  echo "[MOCK] box $@"
  echo -n "$MOCK_BOX_RTESULT"
  exit 0
else
  next_box=$(command -v box | grep -v "$0" | head -n1)
  if [ -x "$next_box" ]; then
    exec "$next_box" "$@"
  else
    echo "box CLI not found for passthrough" >&2
    exit 127
  fi
fi
